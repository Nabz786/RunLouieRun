<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Game.GameOverController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@../Resources/Images/game%20over%20(3).png" />
         </image>
      </ImageView>
      <Button fx:id="restartButton" layoutX="40.0" layoutY="188.0" mnemonicParsing="false" onAction="#initialize" prefHeight="38.0" prefWidth="166.0" text="Button">
         <effect>
            <ImageInput>
               <source>
                  <Image url="@../Resources/Images/restart.png" />
               </source>
            </ImageInput>
         </effect>
      </Button>
      <Button fx:id="mainMenuButton" layoutX="387.0" layoutY="188.0" mnemonicParsing="false" onAction="#initialize" prefHeight="38.0" prefWidth="179.0" text="Button">
         <effect>
            <ImageInput>
               <source>
                  <Image url="@../Resources/Images/mainMenu_text%20(1).png" />
               </source>
            </ImageInput>
         </effect>
      </Button>
   </children>
</AnchorPane>